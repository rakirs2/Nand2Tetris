/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, in = a, out = notA);
    Not(in = b, in = b, out = notB);
    Nand(a = notA, b = b, out = nandOne);
    Nand(a = a, b = notB , out = nandTwo);
    Nand(a= nandOne, b = nandTwo, out= out);
}